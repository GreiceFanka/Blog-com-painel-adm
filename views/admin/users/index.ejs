<%- include ('../../partials/header.ejs');%>
<%- include ('../../partials/navbar.ejs');%>

<div class="container">
    <br>
    <h3>Usuários</h3>
    <a class="btn btn-primary" href="/admin/users/create">Criar novo usuário</a>
    <br>
    <br>
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user =>{ %>
                <tr>
                    <td><%= user.id %></td>
                    <td><%= user.email %></td>
                
                </tr>
            <%})%>
           
        </tbody>
    </table>
</div>

<%- include('../../partials/footer.ejs');%>